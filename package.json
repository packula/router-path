{
  "name": "@snout/router-path",
  "version": "0.0.0",
  "description": "A simple, light-weight, type-safe router path implementation",
  "keywords": [
    "path",
    "route",
    "router",
    "routing"
  ],
  "repository": "snout-router/router-path",
  "bugs": "https://github.com/snout-router/router-path/issues",
  "homepage": "https://snout.dev/router-path",
  "author": "Erin Millard <ezzatron@gmail.com>",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "typings": "dist/index.d.ts",
  "exports": {
    "import": "./dist/index.es.js",
    "require": "./dist/index.js"
  },
  "sideEffects": false,
  "files": [
    "/dist/"
  ],
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "prepare": "rollup --config rollup.config.js",
    "size-limit-build": "rollup --config rollup.config.js",
    "size-limit-clean": "rm -rf dist"
  },
  "dependencies": {
    "@snout/regexp": "^0.2.1"
  },
  "devDependencies": {
    "@size-limit/preset-small-lib": "^5.0.3",
    "@snout/eslint-config": "^2.0.0",
    "@snout/jest-config": "^1.0.2",
    "@snout/rollup-config": "^1.1.2",
    "@snout/tsconfig": "^1.0.3",
    "@typescript-eslint/eslint-plugin": "^4.18.0",
    "@typescript-eslint/parser": "^4.18.0",
    "codecov": "^3.0.2",
    "eslint": "^7.21.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^25.2.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "size-limit": "^5.0.3",
    "typescript": "^4.2.3"
  }
}
